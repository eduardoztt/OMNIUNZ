<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ContaActivity">





    <Button
        android:id="@+id/iconButtonBack"
        style="?attr/materialIconButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:icon="@drawable/baseline_arrow_back_24"
        app:iconTint="#000000"
        app:iconSize="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Conta"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:textSize="32sp"
        app:layout_constraintBottom_toBottomOf="@+id/iconButtonBack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iconButtonBack" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/textFieldUsuario"
        app:layout_constraintStart_toStartOf="@+id/textFieldUsuario" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textFieldUsuario"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="90dp"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toBottomOf="@+id/view2"
        tools:layout_editor_absoluteX="34dp">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/emailText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:enabled="false"
            android:textStyle="bold" />

    </com.google.android.material.textfield.TextInputLayout>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UsuÃ¡rio"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/textFieldUrio"
        app:layout_constraintStart_toStartOf="@+id/textFieldUrio" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textFieldUrio"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="90dp"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toBottomOf="@+id/textFieldUsuario"
        tools:layout_editor_absoluteX="34dp">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/usuarioText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:enabled="false"
            android:textStyle="bold" />

    </com.google.android.material.textfield.TextInputLayout>

    <Button
        android:id="@+id/iconButtonEditUsuario"
        style="?attr/materialIconButtonOutlinedStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="14dp"
        app:icon="@drawable/baseline_edit_24"
        app:iconTint="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/textFieldUrio"
        app:layout_constraintStart_toEndOf="@+id/textFieldUrio"
        app:layout_constraintTop_toTopOf="@+id/textFieldUrio" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Senha"
        android:textSize="20sp"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/textFieUrio"
        app:layout_constraintStart_toStartOf="@+id/textFieUrio" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textFieUrio"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="90dp"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toBottomOf="@+id/textFieldUrio"
        tools:layout_editor_absoluteX="34dp">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/senhaText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:enabled="false"
            android:textStyle="bold" />

    </com.google.android.material.textfield.TextInputLayout>

    <Button
        android:id="@+id/iconButtonEditSenha"
        style="?attr/materialIconButtonOutlinedStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="14dp"
        app:icon="@drawable/baseline_edit_24"
        app:iconTint="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/textFieUrio"
        app:layout_constraintStart_toEndOf="@+id/textFieUrio"
        app:layout_constraintTop_toTopOf="@+id/textFieUrio"/>







</androidx.constraintlayout.widget.ConstraintLayout>